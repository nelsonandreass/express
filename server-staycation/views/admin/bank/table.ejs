<div class="card shadow mb-4">
    <div class="card-header py-3">
        <a href="#" data-toggle="modal" data-target="#modalcetegory" class="btn btn-primary">
            Add
        </a>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Name Bank</th>
                        <th>Nomor Rekening</th>
                        <th>Name</th>
                        <th>Action</th>
                    </tr>   
                </thead>
                <tbody>
                    <% for(let i = 0 ; i < banks.length ; i++){%>
                        <tr>
                            <td><%= i+1 %></td>
                            <td><%= banks[i].nameBank %></td>
                            <td><%= banks[i].nomorRekening %></td>
                            <td><%= banks[i].name %></td>
                            <td>
                                action
                            </td>
                        </tr>
                    <%}%>
                </tbody>
            </table>
        </div>
    </div>
</div>